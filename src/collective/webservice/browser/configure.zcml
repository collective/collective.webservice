<configure
   xmlns="http://namespaces.zope.org/zope"
   xmlns:browser="http://namespaces.zope.org/browser"
   i18n_domain="collective.webservice">

    <browser:page
        for="*"
        name="webservice_view"
        class=".views.WSView"
        permission="zope2.Public"
        allowed_attributes="webservice_caller
                            webservice_caller_axis
                            webservice_caller_proxy
                            call_webservice"
        />

</configure>
